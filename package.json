{
  "name": "beamcoder-prebuild",
  "version": "0.6.10-rc.9",
  "description": "Node.js native bindings to FFmpeg.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "install": "prebuild-install --runtime napi",
    "test": "tape test/*.js",
    "lint": "eslint **/*.js",
    "lint-html": "eslint **/*.js -f html -o ./reports/lint-results.html",
    "lint-fix": "eslint --fix **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/descriptinc/beamcoder.git"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "files": [
    "types/**",
    "beamstreams.js",
    "index.d.ts",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "FFmpeg",
    "libav",
    "video",
    "audio",
    "sound",
    "encode",
    "decode",
    "transcode",
    "N-API"
  ],
  "author": "Streampunk Media Ltd",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/descriptinc/beamcoder/issues"
  },
  "homepage": "https://github.com/descriptinc/beamcoder#readme",
  "dependencies": {
    "bindings": "^1.5.0",
    "patch-package": "^6.2.2",
    "prebuild-install": "^6.0.0",
    "segfault-handler": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "prebuild": "^10.0.1",
    "tape": "^5.0.1"
  },
  "gypfile": true
}
